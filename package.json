{
  "name": "@vodyani/vodyani",
  "license": "MIT",
  "version": "8.5.1",
  "author": "ChoGathK",
  "description": "ðŸš€ A fantastic solution for nodejs server-side apps.",
  "homepage": "https://github.com/vodyani/vodyani#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vodyani/vodyani.git"
  },
  "bugs": {
    "url": "https://github.com/vodyani/vodyani/issues"
  },
  "keywords": [
    "vodyani",
    "nodejs",
    "nestjs"
  ],
  "scripts": {
    "local": "npm i && npm i @vodyani/eslint-config -D && npx husky install",
    "build": "rm -rf dist && tsc",
    "dev": "ts-node src/main",
    "start": "npm run build && node dist/main",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src ./test && tsc",
    "lint:fix": "eslint --fix --ext .ts src/ test/",
    "lint:html-report": "eslint ./src ./test -f html -o eslint-report.html",
    "lint:json-report": "eslint ./src ./test -f json -o eslint-report.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "@vodyani"
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/test/.spec.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "./node_modules/(?!(lodash-es|other-es-lib))"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^8.4.7",
    "@nestjs/core": "^8.4.7",
    "@nestjs/platform-express": "8.4.7",
    "@vodyani/ark": "^8.8.0",
    "@vodyani/class-decorator": "^8.2.3",
    "@vodyani/core": "^8.10.0",
    "@vodyani/utils": "^8.8.1",
    "@vodyani/winston": "^8.7.0",
    "dayjs": "1.11.5",
    "lodash": "4.17.21",
    "module-alias": "2.2.2",
    "rxjs": "7.8.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.3.0",
    "@commitlint/config-conventional": "16.2.4",
    "@nestjs/testing": "8.4.7",
    "@types/express": "4.17.13",
    "@types/jest": "27.5.2",
    "@types/lodash": "4.14.182",
    "@types/module-alias": "2.0.1",
    "@types/multer": "1.4.7",
    "@types/node": "16.11.56",
    "@types/supertest": "2.0.12",
    "@types/uuid": "^8.3.4",
    "@vodyani/eslint-config": "^1.1.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "supertest": "6.2.4",
    "ts-jest": "27.1.5",
    "ts-node": "10.9.1",
    "typescript": "4.8.2"
  }
}
